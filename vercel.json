{
  "version": 2,
  "functions": {
    "api/**/*.js": { "maxDuration": 30, "memory": 1024, "runtime": "nodejs22.x" }
  },
  "rewrites": [
    { "source": "/proxy", "destination": "/api/proxy" },
    { "source": "/crypto", "destination": "/api/crypto" },
    { "source": "/excel", "destination": "/api/excel" }
  ]
}


